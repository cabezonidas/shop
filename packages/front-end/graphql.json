{
  "overwrite": true,
  "schema": "http://localhost:8899/graphql",
  "documents": "src/**/*.graphql",
  "generates": {
    "src/generated/graphql.tsx": {
      "plugins": ["typescript", "typescript-operations", "typescript-react-apollo"],
      "config": {
        "withHOC": false,
        "withComponent": false,
        "withHooks": true
      }
    }
  }
}
