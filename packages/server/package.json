{
  "name": "@cabezonidas/shop-server",
  "version": "0.0.1",
  "description": "backend project for shop",
  "keywords": [
    "express",
    "nosql",
    "graphql"
  ],
  "author": "Cabe <sebastian.scd@gmail.com>",
  "homepage": "https://github.com/cabezonidas/shop#readme",
  "license": "ISC",
  "main": "lib/server.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabezonidas/shop.git"
  },
  "scripts": {
    "tsc": "tsc",
    "prebuild": "tslint -c ../../tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node -r ts-node/register ./src/server.ts",
    "start-server": "nodemon",
    "typeorm": "./node_modules/.bin/typeorm",
    "start-lambda": "npm run sls offline",
    "test": "echo \"No tests to run\""
  },
  "bugs": {
    "url": "https://github.com/cabezonidas/shop/issues"
  },
  "typings": "lib/server.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "apollo-server-express": "^2.12.0",
    "aws-lambda": "^1.0.5",
    "aws-sdk": "^2.663.0",
    "aws-serverless-express": "^3.3.8",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "googleapis": "^49.0.0",
    "graphql": "^14.5.8",
    "i18next": "^19.4.3",
    "i18next-express-middleware": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "middy": "^0.36.0",
    "mongoose": "^5.9.10",
    "reflect-metadata": "^0.1.13",
    "serverless": "^1.68.0",
    "serverless-domain-manager": "^3.3.2",
    "serverless-dotenv-plugin": "^2.4.2",
    "serverless-offline": "^6.1.4",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-node": "^8.9.0",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.22"
  },
  "devDependencies": {
    "@types/apollo-upload-client": "^8.1.3",
    "@types/aws-serverless-express": "^3.3.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.0.7",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mongoose": "^5.5.35",
    "@types/node": "^13.1.1",
    "nodemon": "^2.0.2",
    "typescript": "^3.7.4"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
